@inherits RazorPageTemplate

@functions {
    public override void Configure()
    {
        TemplateId = new Guid("0526ad34-c540-418e-8c23-0eec2a8da2ce");
        TemplateTitle = "Page";
        Layout = "MasterLayout.cshtml";
    }

    [Placeholder(Id = "content", Title = "Content", IsDefault = true)]
    public XhtmlDocument Content { get; set; }
}

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
</head>
<body class="page">
    @PageTemplateFeature("Content Start")
    @this.Function("Layout.Helpers.InjectContainerElements", new { Document = Content, InjectedContainerClassName = "container", DontApplyClassNameList = "container,container-fluid,c1-fullwidth" })
</body>
</html>