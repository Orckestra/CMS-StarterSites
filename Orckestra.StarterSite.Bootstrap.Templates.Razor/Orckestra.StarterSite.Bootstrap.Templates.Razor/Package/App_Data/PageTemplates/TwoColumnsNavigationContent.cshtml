@inherits RazorPageTemplate
@functions {
    public override void Configure()
    {
        TemplateId = new Guid("e3851f7a-3f4b-4eda-9708-07c3b6020e08");
        TemplateTitle = "2 columns, navigation and content";
        Layout = "MasterLayout.cshtml";
    }

    [Placeholder(Id = "content", IsDefault = true)]
    public XhtmlDocument Content { get; set; }

    [Placeholder(Id = "jumbotron")]
    public XhtmlDocument Jumbotron { get; set; }
}
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        @*you can add template specific head elements here*@
    </head>
    <body>
        @PageTemplateFeature("Content Start")
        @Function("Layout.Jumbotron", new { CustomContent = this.Jumbotron })
        <div class="row">
            <div class="col-xs-12 col-sm-4 col-md-3">
                <div class="well nav-sidebar hidden-print">
                    @PageTemplateFeature("Navigation Column")
                </div>
            </div>
            <div class="col-xs-12 col-sm-8 col-md-9">
                @Markup(Content)
            </div>
        </div>
    </body>
</html>